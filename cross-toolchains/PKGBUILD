build() {

  echo -e "\nMake Binutils\n"
  mkdir -pv binutils
  cp -avr BINUTILS-TOOLCHAINS-PKGBUILD binutils/PKGBUILD
  cd binutils
  makepkg -f
  cd ..
  rm -rfv binutils
}

package() {
  echo -e "No Install"
  cd ..
}
