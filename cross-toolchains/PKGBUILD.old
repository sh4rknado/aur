build() {

  echo -e "\nInstall Binutils PASS_1\n"
  mkdir -pv binutils
  cp -avr binutils-2.35.tar.xz binutils
  cp -avr BINUTILS_PKGBUILD binutils/PKGBUILD
  cd binutils
  makepkg -f
  cd ..
  rm -rfv binutils

  echo -e "\nInstall GCC PASS_1\n"
  mkdir -pv gcc
  echo -e "\n\n"
  pwd
  echo -e "\n\n"

  cp -avr gcc-10.2.0.tar.xz gcc
  cp -avr mpfr-4.1.0.tar.xz gcc
  cp -avr gmp-6.2.0.tar.xz gcc
  cp -avr mpc-1.1.0.tar.gz gcc
  cp -avr GCC_PKGBUILD gcc/PKGBUILD
  cd gcc
  makepkg -f
  cd ..
  rm -rfv gcc

  echo -e "\nInstall lINUX Headers\n"
  mkdir -pv linux
  cp -avr linux-5.8.1.tar.xz linux
  cp -avr LINUXHEADER_PKGBUILD linux/PKGBUILD
  cd linux
  makepkg -f
  cd ..
  rm -rfv linux

  echo -e "\nInstall Glibc Headers\n"
  mkdir -pv glibc
  cp -avr  glibc-2.32.tar.xz glibc
  cp -avr  glibc-2.32-fhs-1.patch glibc
  cp -avr  GLIBC_PKGBUILD glibc/PKGBUILD
  cd glibc
  makepkg -f
  cd ..
  rm -rfv glibc

  echo -e "\nInstall LIBSTDC Headers\n"
  mkdir -pv libstc
  cp -avr  gcc-10.2.0.tar.xz libstc
  cp -avr  LIBSTDC_PKGBUILD libstc/PKGBUILD
  cd libstc
  makepkg -f
  cd ..
  rm -rfv libstc
}

package() {
  echo -e "No Install"
  cd ..
}
